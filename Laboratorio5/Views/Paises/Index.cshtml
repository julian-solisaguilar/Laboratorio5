@model List<Laboratorio5.Models.PaisModel>;

@{
  ViewData["Title"] = "Lista de Paises";
}

<h1>@ViewBag.MainTitle</h1><br><br><br>

<div>
  <table class = "table">
    <thead>
      <tr>
        <th>Id</th>
        <th>Nombre</th>
        <th>Idioma</th>
        <th>Continente</th>
      </tr>
    </thead>
    <tbody>
      @foreach (var pais in Model)
      {
        <tr>
          <td>@pais.Id</td>
          <td>@pais.Nombre</td>
          <td>@pais.Idioma</td>
          <td>@pais.Continente</td>
        </tr>
      }
    </tbody>
  </table>
</div>

